[data-component='WhiteBoard']{
  width :40%;
  background:rgba(255,255,255,1);
  box-shadow:0px 1px 5px 0px rgba(31,32,33,0.3);
  border-radius:0px 3px 3px 0px;
  >.WhiteBoard_header{
    height 50px
    width:90%;
    margin auto
    margin-top 20px
    border-bottom #A0A0A0FF solid 1px

    >.WhiteBoard_text{
      font-size:18px;
      font-family:Microsoft YaHei;
      font-weight:bold;
      color:rgba(83,122,232,1);
      line-height:11px;    }

  }

}