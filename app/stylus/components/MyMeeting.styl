[data-component='MyMeeting']{
  margin-top :100px;
  height :100%;
  width :100%;
  overflow: auto;

  >._header {
    width: 90%;
    margin  auto
    font-size:20px;
    font-family:Microsoft YaHei;
    font-weight:bold;
    color:rgba(83,83,83,1);

    margin-bottom 20px
    margin-top 10px
  }
  >.add_meeting {
    margin auto
    display flex
    justify-content space-between
    width:90%;
    background:rgba(255,255,255,1);
    box-shadow:0px 2px 10px 0px rgba(28,45,52,0.2);
    >.add_meeting_p1{
      width :300px
      > span{
        width:54px;
        height:8px;
        font-size:16px;
        font-family:Microsoft YaHei;
        font-weight:400;
        color:rgba(83,83,83,1);
        line-height:50px;
        margin-left:30px
      }
    }
    >.add_meeting_p2{
      width: 175px
      > a{
        width:300px;
        height:8px;
        font-size:16px;
        font-family:Microsoft YaHei;
        font-weight:400;
        color #0d55c7;
        line-height:80px;
        margin-left:30px
      }

    }

  }

  >._calendar{
    width 90%;
    margin auto
  }

}